		<![CDATA[SELECT
	tb_beli.id_beli,
	tb_beli.tgl_faktur,
	tb_beli.no_faktur,
	tb_beli.tgl_beli,
	tb_supplier.nama_supplier AS nama_supplier,
	tb_apoteker.nama_apoteker AS nama_apoteker,
	tb_beli.total_harga 
